{
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jest": true,
    "serviceworker": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "semi": false,
        "singleQuote": true,
        "bracketSpacing": false
      }
    ],
    "react/react-in-jsx-scope": false,
    "react/jsx-no-undef": false,
    "react/prop-types": false,
    "react/display-name": false
  },
  "parser": "babel-eslint"
}
